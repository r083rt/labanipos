{
  "name": "labani_pos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "realm:delete": "rm default.realm default.realm.lock default.realm.note",
    "realm:extract": "adb exec-out run-as com.labani_pos cat files/default.realm > labani_pos.realm",
    "realm:open": "npm run realm:extract && open -F default.realm",
    "clean": "cd android && gradlew clean",
    "buildQA": "cd android && gradlew assembleDebug",
    "buildRelease": "cd android && gradlew app:assembleRelease",
    "buildPRD": "cd android && gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "bundleQA": "react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundlePRD": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@brooons/react-native-bluetooth-escpos-printer": "^1.2.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/checkbox": "^0.5.16",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@shopify/flash-list": "^1.4.3",
    "axios": "^1.4.0",
    "lottie-react-native": "^5.1.6",
    "moment": "^2.29.4",
    "native-base": "^3.4.28",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-autocomplete-input": "^5.3.2",
    "react-native-date-picker": "^4.2.13",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-pager-view": "^6.2.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.20.0",
    "react-native-super-grid": "^5.0.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-text-avatar": "^1.0.7",
    "react-native-vector-icons": "^9.2.0",
    "realm": "^11.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
